{% extends 'base.html.twig' %}

{% block body %}
    {{ form_start(form, {'attr': {'id': 'user-forms'}})}}
        {{ form_row(form.username) }}
        {{ form_row(form.pseudo) }}
        {{ form_row(form.email) }}
        {{ form_row(form.dateOfBirth) }}
        {{ form_row(form.password.first) }}
        {{ form_row(form.password.second) }}
    {{ form_end(form) }}

{% endblock %}
